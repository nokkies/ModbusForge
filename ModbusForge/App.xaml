<Application x:Class="ModbusForge.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ModbusForge"
             xmlns:converters="clr-namespace:ModbusForge.Converters;assembly=ModbusForge"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
            <converters:BooleanToConnectedConverter x:Key="BooleanToConnectedConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
